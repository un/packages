{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "noUncheckedIndexedAccess": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "verbatimModuleSyntax": true,
    "lib": ["ESNext"],
    "moduleResolution": "Bundler",
    "alwaysStrict": true,
    "strict": true,
    "module": "ESNext",
    "target": "ESNext"
  }
}
